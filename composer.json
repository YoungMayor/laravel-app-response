{
    "name": "youngmayor/app-response",
    "description": "A laravel package for providing unified easy to use response to our laravel applicatons ",
    "keywords": [
        "youngmayor",
        "app-response"
    ],
    "homepage": "https://github.com/youngmayor/app-response",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Meyoron Aghogho",
            "email": "youngmayor.dev@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "YoungMayor\\AppResponse\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "YoungMayor\\AppResponse\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "YoungMayor\\AppResponse\\AppResponseServiceProvider"
            ],
            "aliases": {
                "AppResponse": "YoungMayor\\AppResponse\\AppResponseFacade"
            }
        }
    }
}
